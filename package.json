{
  "name": "lambda-adapter-demo",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build && tsc --project ./tsconfig.server.json && tsc-alias --project ./tsconfig.server.json",
    "dev": "cross-env NODE_ENV=development tsx --watch-path ./server/index.ts ./server/index.ts",
    "format": "prettier --ignore-path .gitignore --ignore-unknown --cache --cache-location node_modules/.cache/prettiercache --write .",
    "lint": "eslint --ignore-path .gitignore --no-error-on-unmatched-pattern --cache --cache-location node_modules/.cache/eslint --fix . && npm run format",
    "start": "cross-env NODE_ENV=production node ./fastify-server-build/index.js",
    "typecheck": "tsc",
    "cdk:deploy:infra:it": "cdk deploy InfraStack-it -c env=it",
    "cdk:synth:infra:it": "cdk synth InfraStack-it -c env=it",
    "cdk:deploy:lambda:it": "cdk deploy LambdaStack-it -c env=it",
    "cdk:synth:lambda:it": "cdk synth LambdaStack-it --require-approval never -c env=it"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.665.0",
    "@aws-sdk/lib-dynamodb": "^3.665.0",
    "@fastify/autoload": "^6.0.1",
    "@fastify/middie": "^8.3.1",
    "@fastify/static": "^7.0.4",
    "@mcansh/remix-fastify": "3.4.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/server-runtime": "^2.12.1",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "fastify": "^4.28.1",
    "get-port": "^7.1.0",
    "isbot": "^5.1.11",
    "lucide-react": "^0.447.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "source-map-support": "^0.5.21",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.1",
    "@remix-run/eslint-config": "^2.12.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "aws-cdk": "2.160.0",
    "aws-cdk-lib": "2.160.0",
    "constructs": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.13",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.2",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
